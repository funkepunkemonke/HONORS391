getwd()
write.table(pirates, file = "pirates.csv", sep = ",", row.names = F )
mydata <- read.table(file = 'pirates.txt',    # file is in a data folder in my working directory
                     sep = '\t',                  # file is tab--delimited
                     header = TRUE,               # the first row of the data is a header row
                     stringsAsFactors = FALSE)    # do NOT convert strings to factors!!

identical(mydata, pirates)
pirates<-pirates

install.packages("compareDF")
library(compareDF)

compare_df(mydata, pirates)

